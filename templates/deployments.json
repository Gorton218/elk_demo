{
    "index_patterns": [
        "logstash-*"
    ],
    "mappings": {
        "dynamic_templates": [
            {
                "strings": {
                    "match_mapping_type": "string",
                    "match": "*",
                    "mapping": {
                        "type": "text"
                    }
                }
            }
        ],
        "properties": {
            "version": {
                "type": "keyword"
            },
            "description": {
                "type": "text"
            },
            "timestamp": {
                "type": "date"
            }
        }
    }
}
